<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="http-equiv" content="Content-type: text/html; charset=UTF-8" />
    <script charset="utf-8" type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js"></script>
    <link rel="stylesheet" type="text/css" href="/scss/style.min.css" />
    <title>@ViewBag.Title</title>
</head>
<body>
    <header>
        <div class="content">
            <div class="account-data">
                <a class="logout-panel-button">
                    <img src="/images/test/1671822124_kartinkin-net-p-kartinki-leto-estetika-oboi-14.jpg" />
                    <p>@User.Identity.Name</p>
                </a>
                <div class="logout-panel">
                    <a href="login/logout">Выйти</a>
                </div>
            </div>
            <div class="info">
                <div class="month-info">
                </div>
                <div class="year-info">
                    <a class="previous-year"><img src="/images/icons/arrow.png" /></a>
                    <p class="total-year"></p>
                    <a class="next-year"><img src="/images/icons/arrow.png" /></a>
                </div>
            </div>
            <div class="today-button">
                <a href="/home">Сегодня</a>
            </div>
        </div>
    </header>
    <div class="main-content">
        @RenderBody()
    </div>
    <footer>
    </footer>
</body>
</html>

<script type="text/javascript" src="/scripts/heademenucontrol.js" charset="utf-8"></script>